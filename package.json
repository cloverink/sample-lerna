{
  "name": "@cloverink/sample-lerna",
  "version": "1.18.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "www/*"
  ],
  "scripts": {
    "start": "lerna run start --scope @cloverink/sample-lerna-example-app",
    "build": "lerna run build --scope @cloverink/sample-lerna-helloworld",
    "lerna bootstrap": "lerna bootstrap",
    "publish:release": "lerna publish --conventional-commits --no-verify-access --yes"
  },
  "devDependencies": {
    "lerna": "^5.1.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
