{
  "version": "1.27.0",
  "npmClient": "npm",
  "command": {
    "version": {
      "preid": "next",
      "ignoreChanges": [
        "ignored-file",
        "**/__tests__/**",
        "**/*.md",
        "lerna.json"
      ],
      "conventionalCommits": true,
      "commitHooks": false,
      "message": "chore(release): Publish %s"
    }
  }
}
